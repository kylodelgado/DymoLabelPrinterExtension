{
  "manifest_version": 3,
  "name": "DYMO Label Printer",
  "version": "1.0",
  "description": "Print DYMO labels with custom text and SKU",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://127.0.0.1:41951/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "DYMO Label Printer"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 